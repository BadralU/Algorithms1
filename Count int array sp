public class Count8 {

	public static void main(String[] args) {
		int []array1 = {7,8,6,5,6,4,7,8,8,8,5};
		System.out.println(arrayCount(array1));
	}
	
	public static int arrayCount(int arr[]) {
		int count=0;
		for(int i=0; i<arr.length; i++) {
			if(arr[i]==8) {
				count++;
			}
		}return count;
	}
}

public class Class2 {

	public static void main(String[] args) {
		int ar[] = {1,5,6,6,7,45,6,6,7};
		System.out.println(arrayC(ar));

	}
	public static int arrayC(int[] nums) {
		  int count=0;
		  for(int i=0; i<nums.length-1; i++){
		    if(nums[i]==6){
		      if(nums[i+1]==6 || nums[i+1]==7){
		      count++;
		      } 
		    }
		  }return count;
		}

}

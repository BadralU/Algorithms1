public class ShuffleTheArray {
	public static void main(String[] args) {	
		
		int []nums  = {1,2,3,4,5,6,7,8};
		int n=3;
		
	}
}
	class Solution{
		public int[] shuffle(int[]nums, int n) {
			int []res = new int[2*n];
			int i=0;
			int j=n;
			int curr=0;
			while(i<n) {
				res[curr]=nums[i];
				curr++;
				i++;
				res[curr]= nums[j];
				curr++;
				j++;
			}
			
		return res;
		}
	}

public class IntArr {

	public static void main(String[] args) {
	int[] ar = {4,3,1,2,3};
	System.out.println(arr(ar));;
	}

	public static boolean arr(int[] intArr) {
		  for(int i=0; i<intArr.length-2; i++){
		    if(intArr[i]==1 && intArr[i+1]==2 && intArr[i+2]==3){
		      return true;
		    }
		  }return false;
		  
		}
}


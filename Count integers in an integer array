public class CountIntArray {

	public static void main(String[] args) {
		
		int ar[]= {4,5,9,3,9,2,9,9};
System.out.println(arrayCount(ar));
	}
	public static int arrayCount(int[]nums) {
		  int count =0;
		  for(int i=0; i<nums.length; i++){
		    if(nums[i]==9){
		      count++;
		    }
		  }return count;
	}
}
